diff --git a/frusers/views.py b/frusers/views.py
index 9169b7b..f024d99 100755
--- a/frusers/views.py
+++ b/frusers/views.py
@@ -56,6 +56,9 @@ def fruser_data(request):
             fruser['Index'] = index + 1
             index += 1
             fruser['Group'] = ', '.join(group['Name'] for group in Frusergroups.objects.filter(frusergroupmember__FRUserOId__OId = fruser['OId']).values('Name'))
+            fruser['FR'] = 'archived' if fruser['FaceID1'] or fruser['FaceID2'] or fruser['FaceID3'] else 'unarchived'
+            fruser['RFID'] = 'archived' if fruser['RFIDCard'] else 'unarchived'
+            fruser['FP'] = 'archived' if fruser['FingerprintID1'] != 'x00' or fruser['FingerprintID2'] != 'x00' else 'unarchived'
         return JsonResponse({'total': fruser_count, 'rows': frusers})
     except:
         return JsonResponse({'total': 0, 'rows': []})
@@ -77,6 +80,9 @@ def fruser_edit_view(request):
     try:
         fruser = Frusers.objects.get(OId = request.GET['OId']).as_json()
         fruser['Age'] = fruser['Age'] or ''
+        fruser['FR'] = 'archived' if fruser['FaceID1'] or fruser['FaceID2'] or fruser['FaceID3'] else 'unarchived'
+        fruser['RFID'] = 'archived' if fruser['RFIDCard'] else 'unarchived'
+        fruser['FP'] = 'archived' if fruser['FingerprintID1'] != 'x00' or fruser['FingerprintID2'] != 'x00' else 'unarchived'
         for Face in {'Face1', 'Face2'}:
             if fruser[Face]:
                 fruser[Face] = 'media/{0}'.format(fruser[Face])
