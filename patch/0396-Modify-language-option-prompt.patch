Subject: [PATCH 396/443] Modify language option prompt

---
 br06/settings.py               | 2 +-
 templates/device_settings.html | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/br06/settings.py b/br06/settings.py
index 1ed3cc8..43864f6 100755
--- a/br06/settings.py
+++ b/br06/settings.py
@@ -183,7 +183,7 @@ _ = lambda s: s
 LANGUAGES = (
     ('en-us', _('English')),
     ('zh-TW', _('繁體中文')),
-    ('zh-CN', _('筒体中文')),
+    ('zh-CN', _('简体中文')),
 )
 
 LANGUAGE_CODE = 'en-us'
diff --git a/templates/device_settings.html b/templates/device_settings.html
index 86e43f5..1b05161 100755
--- a/templates/device_settings.html
+++ b/templates/device_settings.html
@@ -297,7 +297,7 @@
                                         <div class="col-md-3">
                                             <select class="form-control" name="Language">
                                                 <option value="en">English</option>
-                                                <option value="zh-cn">筒体中文</option>
+                                                <option value="zh-cn">简体中文</option>
                                                 <option value="zh-tw">繁體中文</option>
                                             </select>
                                         </div>
